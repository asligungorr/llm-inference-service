{
  "id": null,
  "title": "LLM Inference Service",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests (Total)",
      "targets": [
        {
          "expr": "sum by (path) (http_requests_total)",
          "legendFormat": "{{path}}"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, path))",
          "legendFormat": "{{path}}"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Celery Task Duration (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(celery_task_duration_seconds_bucket[5m])) by (le, task))",
          "legendFormat": "{{task}}"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Celery Task Failures",
      "targets": [
        {
          "expr": "sum by (task) (celery_task_failures_total)",
          "legendFormat": "{{task}}"
        }
      ],
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Redis Memory Usage",
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "used_bytes"
        }
      ],
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 }
    }
  ]
}
